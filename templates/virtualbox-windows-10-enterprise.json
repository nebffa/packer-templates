{
    "builders": [
        {
            "type": "virtualbox-iso",
            "vboxmanage": [
                [
                    "modifyvm",
                    "{{.Name}}",
                    "--memory",
                    "2048"
                ],
                [
                    "modifyvm",
                    "{{.Name}}",
                    "--vram",
                    "48"
                ],
                [
                    "modifyvm",
                    "{{.Name}}",
                    "--cpus",
                    "2"
                ]
            ],
            "guest_additions_mode": "{{ user `guest_additions_mode` }}",
            "guest_os_type": "Windows10_64",
            "headless": "{{ user `headless` }}",
            "iso_url": "{{ user `iso_url` }}",
            "iso_checksum": "{{ user `iso_checksum` }}",
            "iso_checksum_type": "md5",
            "communicator": "winrm",
            "winrm_username": "vagrant",
            "winrm_password": "vagrant",
            "winrm_timeout": "12h",
            "shutdown_command": "A:/packer_shutdown.bat",
            "shutdown_timeout": "15m",
            "floppy_files": [
                "unattend_files/windows_10/Autounattend.xml",
                "unattend_files/windows_10/Post_Autounattend.xml",
                "scripts/winrm.ps1",
                "scripts/packer_shutdown.bat"
            ]
        }
    ],
    "post-processors": [
        {
            "type": "vagrant",
            "keep_input_artifact": true,
            "output": "{{env `IMAGES_DIR`}}/windows-10-{{.Provider}}.box"
        }
    ],
    "variables": {
        "guest_additions_mode": "attach",
        "headless": "true",
        "iso_checksum": "f4f85d77516721d9a19ca866172a5ecb",
        "iso_url": "{{env `IMAGES_DIR`}}/Windows_10_Enterprise.iso"
    }
}
